{
  "name": "Andromeda",
  "type": "module",
  "version": "1.0.0",
  "description": "Andromeda is Portals Authentication and Authorization layer"  ,
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx src/index.ts",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "eslint:check": "eslint \"src/**/*.ts\"",
    "eslint:fix": "eslint \"src/**/*.ts\" --fix",
    "fix": "npm run eslint:fix && npm run prettier:fix",
    "migration:up": "tsx src/scripts/migrate.ts",
    "migration:down": "tsx src/scripts/migrateDown.ts",
    "migration:generate": "tsx src/scripts/generateMigration.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "pnpm run eslint:check",
      "pnpm run prettier:check",
      "git add"
    ]
  },
  "dependencies": {
    "@farcaster/hub-nodejs": "^0.6.3",
    "@noble/ed25519": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "kysely": "^0.24.2",
    "pg": "^8.10.0",
    "tweetnacl": "^1.0.3",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.12.6"
  }
}
